{
  "presets": ["react-native"],
  "plugins": [
    ["@babel/plugin-syntax-optional-chaining"],
    ["transform-inline-environment-variables"],
    [
      "module-resolver", 
      {
        "cwd": "babelrc",
        "root": ["./src"],
        "extensions": [".ts", ".tsx", ".js", ".jsx", ".ios.js", ".android.js"],
        "alias": {
          "assets": "./assets"
        }
      }
    ]
  ]
}
